  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSourceD</DataSourceName>
        <CommandText>SELECT DimDrivers.Driver_ID, DimDrivers.Driver_Name, AVG(CAST(FactOrderItems.Driver_Rating AS Float)) AS Expr1, COUNT(*) AS Expr2
FROM     DimDrivers INNER JOIN
                  FactOrderItems ON DimDrivers.Driver_ID = FactOrderItems.Driver_ID
GROUP BY DimDrivers.Driver_ID, DimDrivers.Driver_Name
ORDER BY Expr1 DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Driver_ID">
          <DataField>Driver_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Driver_Name">
          <DataField>Driver_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Expr1">
          <DataField>Expr1</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Expr2">
          <DataField>Expr2</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
